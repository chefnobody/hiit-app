<p id="notice"><%= notice %></p>

<h1>Workout Movements</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workout_movements.each do |workout_movement| %>
      <tr>
        <td><%= link_to 'Show', workout_movement %></td>
        <td><%= link_to 'Edit', edit_workout_movement_path(workout_movement) %></td>
        <td><%= link_to 'Destroy', workout_movement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workout Movement', new_workout_movement_path %>
